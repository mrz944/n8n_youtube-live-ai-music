            "parameters": {
                "jsCode": "const fs = require('fs');\n\nlet state = {};\ntry {\n  state = JSON.parse(fs.readFileSync('/home/node/.n8n/music-stream/state.json', 'utf8'));\n} catch(e) {\n  state = { broadcastId: null };\n}\n\nreturn [{ json: state }];"
            }
