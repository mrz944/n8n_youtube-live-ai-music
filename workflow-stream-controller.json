        {
            "id": "b0000001-0000-0000-0000-000000000016",
            "name": "Update State Stopped",
            "type": "n8n-nodes-base.code",
            "typeVersion": 2,
            "position": [
                -620,
                300
            ],
            "parameters": {
                "jsCode": "const fs = require('fs');\n\nconst statePath = '/home/node/.n8n/music-stream/state.json';\nconst tmpPath = statePath + '.tmp';\n\nlet state = {};\ntry {\n  state = JSON.parse(fs.readFileSync(statePath, 'utf8'));\n} catch(e) {\n  state = {};\n}\n\nstate.isActive = false;\nstate.stoppedAt = new Date().toISOString();\n\nfs.writeFileSync(tmpPath, JSON.stringify(state, null, 2));\nfs.renameSync(tmpPath, statePath);\n\nreturn [{ json: { status: 'stopped', broadcastId: state.broadcastId } }];"
            }
        },
        {
            "id": "b0000001-0000-0000-0000-000000000017",
            "name": "Respond - Stop Success",
            "type": "n8n-nodes-base.respondToWebhook",
